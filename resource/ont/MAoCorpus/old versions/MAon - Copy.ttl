#################################################################
#    Classes
#################################################################


###  http://EncodingActs.github.io/annotated/MAon/Form_set
mao:Form_set rdf:type owl:Class ;
             rdfs:subClassOf mao:class:training_method ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:belongs_to ;
                               owl:someValuesFrom mao:class:MA_system
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:has_component ;
                               owl:someValuesFrom mao:class:MA_form
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:practiced_in ;
                               owl:someValuesFrom mao:class:MA_style
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:similar_set_of ;
                               owl:someValuesFrom mao:Form_set
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:type_of ;
                               owl:someValuesFrom mao:class:training_method
                             ] ;
             :name_en "Form set"@en ;
             mao:ontoMA_otherName_en "Kata"@ja ,
                                     "Taolu"@en ;
             rdfs:label "form_set"@en .

###  http://EncodingActs.github.io/annotated/MAon/class:E21-Person
mao:class:E21-Person rdf:type owl:Class ;
                     rdfs:subClassOf owl:Thing ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty mao:creates ;
                                       owl:someValuesFrom mao:class:E22-man_made_obj
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty mao:identified_by ;
                                       owl:someValuesFrom mao:class:E41-appellation
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty mao:lives_in_ts ;
                                       owl:someValuesFrom mao:class:E52-timespan
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty mao:resides_in ;
                                       owl:someValuesFrom mao:class:E53-place
                                     ] ;
                     rdfs:label "E21.Person"@en ;
                     rdfs:seeAlso <https://www.cidoc-crm.org/entity/e21-person/version-7.1> .

###  http://EncodingActs.github.io/annotated/MAon/class:E22-man_made_obj
mao:class:E22-man_made_obj rdf:type owl:Class ;
